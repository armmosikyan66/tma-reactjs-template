{
  "version": 2,
  "rewrites": [
    {
      "source": "/reactjs-template/(.*)",
      "destination": "/reactjs-template/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/reactjs-template/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ]
}
